        "laravel",
        "laravel-package",
        "popper",
        "tippy",
        "tooltip",
        "blade",
        "view"
    ],
    "authors": [
        {
            "name": "Anderson Carpi",
            "email": "kustelac@yahoo.com.br"
        }
    ],
    "minimum-stability": "dev",
    "require": {
    },
